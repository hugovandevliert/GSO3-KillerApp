<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ui.controller.CreatePrivateChatController">

    <Pane fx:id="paneHeader" prefHeight="100.0" prefWidth="1100.0">
        <children>
            <Label fx:id="lblCreateChat" layoutX="50.0" layoutY="30.0" text="Create private chat">
                <font>
                    <Font name="Segoe UI Semibold" size="28.0" />
                </font>
            </Label>
        </children>
    </Pane>

    <Pane fx:id="paneContent" layoutY="100.0" prefHeight="625.0" prefWidth="1100.0">
        <children>
            <ScrollPane fx:id="scrollpaneListedUsers" hbarPolicy="NEVER" maxHeight="625.0" maxWidth="1100.0" prefHeight="625.0" prefWidth="1100.0" style="-fx-background: #FFFFFF;" styleClass="edge-to-edge">
                <content>
                    <VBox fx:id="vboxListedUsers" alignment="TOP_CENTER" fillWidth="false" maxHeight="625.0" maxWidth="1100.0" prefHeight="625.0" prefWidth="1100.0" spacing="25.0" style="-fx-background-color: #FFFFFF;" />
                </content>
            </ScrollPane>
        </children>
    </Pane>

</AnchorPane>
