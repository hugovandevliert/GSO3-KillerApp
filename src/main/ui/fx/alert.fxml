<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane layoutX="250.0" prefHeight="100.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.ui.controller.AlertController">
    <Pane prefHeight="100.0" prefWidth="900.0" style="-fx-background-color: #E0E0E0; -fx-background-radius: 2.5;">
        <children>
          <Label fx:id="lblName" alignment="TOP_LEFT" layoutX="75.0" layoutY="5.0" prefHeight="40.0" prefWidth="750.0" text="name">
             <font>
                <Font name="Segoe UI Semibold" size="24.0" />
             </font>
          </Label>
          <Label fx:id="lblMessage" alignment="CENTER" layoutX="75.0" layoutY="40.0" maxHeight="55.0" maxWidth="750.0" prefHeight="55.0" prefWidth="750.0" />
        </children>
    </Pane>
</AnchorPane>
